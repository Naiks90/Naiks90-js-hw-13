{{#each this}}
   <li class="col-2 col-lg-4 col-md-4">
      <div class="photo-card">
    <img class="img" src="{{webformatURL}}" alt="" width="100%" height="" data-src={{largeImageURL}}/>

        <div class="stats">
          <p class="stats-item">
            <i class="material-icons">thumb_up</i>
            {{likes}}
          </p>
          <p class="stats-item">
            <i class="material-icons">visibility</i>
            {{views}}
          </p>
          <p class="stats-item">
            <i class="material-icons">comment</i>
            {{comments}}
          </p>
          <p class="stats-item">
            <i class="material-icons">cloud_download</i>
            {{downloads}}
          </p>
        </div>
      </div>
    </li>
{{/each}}